@prefix ns: <http://example.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
ns:Anotacion rdf:type owl:Class ;
    rdfs:comment "Clase que representa una anotación." .

ns:Paper rdf:type owl:Class ;
    rdfs:comment "Clase que representa un documento de investigación." .

ns:Persona rdf:type owl:Class ;
    rdfs:comment "Clase que representa a una persona." .

# Data Properties
ns:authorId rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Persona ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa el identificador único de una persona." .

ns:citationVelocity rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:integer ;
    rdfs:comment "Propiedad que representa la velocidad de citación de un documento." .

ns:conceptAnotation rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:Anotacion ;
    rdfs:comment "Propiedad que indica las anotaciones asociadas a un documento." .

ns:doi rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa el DOI (Digital Object Identifier) de un documento." .

ns:hasAbstract rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que indica el abstract de un documento." .

ns:hasAñoPublicacion rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:integer ;
    rdfs:comment "Propiedad que representa el año de publicación de un documento." .

ns:hasCitationVelocity rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:integer ;
    rdfs:comment "Propiedad que representa la velocidad de citación de un documento." .

ns:hasDOI rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa el DOI (Digital Object Identifier) de un documento." .

ns:hasInfluentialCitationCount rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:integer ;
    rdfs:comment "Propiedad que indica el conteo de citas influyentes de un documento." .

ns:hasIntroduccion rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que indica la introducción de un documento." .

ns:hasIsOpenAccess rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Propiedad que indica si un documento es de acceso abierto." .

ns:hasIsPublisherLicensed rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Propiedad que indica si un documento está licenciado por un editor." .

ns:hasKeyWords rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que indica las palabras clave de un documento." .

ns:hasPaperId rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa el ID de un documento." .

ns:hasVenue rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa el lugar de publicación de un documento." .

ns:semanticUrl rdf:type owl:DatatypeProperty ;
    rdfs:domain ns:Persona ;
    rdfs:range xsd:string ;
    rdfs:comment "Propiedad que representa la URL semántica asociada a una persona." .

# Object Properties
ns:autor rdf:type owl:ObjectProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:Persona ;
    rdfs:comment "Propiedad que relaciona un documento con su autor." .

ns:references rdf:type owl:ObjectProperty ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:Paper ;
    rdfs:comment "Propiedad que representa las referencias de un documento." .

# Domain and Range Restrictions
ns:authorId rdfs:domain ns:Persona ;
    rdfs:range xsd:string .

ns:citationVelocity rdfs:domain ns:Paper ;
    rdfs:range xsd:integer .

ns:conceptAnotation rdfs:domain ns:Paper ;
    rdfs:range ns:Anotacion .

ns:doi rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasAbstract rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasAñoPublicacion rdfs:domain ns:Paper ;
    rdfs:range xsd:integer .

ns:hasCitationVelocity rdfs:domain ns:Paper ;
    rdfs:range xsd:integer .

ns:hasDOI rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasInfluentialCitationCount rdfs:domain ns:Paper ;
    rdfs:range xsd:integer .

ns:hasIntroduccion rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasIsOpenAccess rdfs:domain ns:Paper ;
    rdfs:range xsd:boolean .

ns:hasIsPublisherLicensed rdfs:domain ns:Paper ;
    rdfs:range xsd:boolean .

ns:hasKeyWords rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasPaperId rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:hasVenue rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:semanticUrl rdfs:domain ns:Persona ;
    rdfs:range xsd:string .

ns:autor rdfs:domain ns:Paper ;
    rdfs:range ns:Persona .

ns:references rdfs:domain ns:Paper ;
    rdfs:range ns:Paper .

# Cardinality Restrictions

# Cardinality Restrictions
ns:Paper owl:hasAbstract xsd:string[1,1] ;
         owl:hasIntroduccion xsd:string[1,1] ;
         owl:hasKeyWords xsd:string[1,*] ;
         owl:hasCitationVelocity xsd:integer[1,1] ;
         owl:hasDOI xsd:string[1,1] ;
         owl:hasPaperId xsd:string[1,1] ;
         owl:hasVenue xsd:string[1,1] ;
         owl:hasAñoPublicacion xsd:integer[1,1] ;
         owl:hasInfluentialCitationCount xsd:integer[0,1] ;
         owl:hasIsOpenAccess xsd:boolean[1,1] ;
         owl:hasIsPublisherLicensed xsd:boolean[1,1] ;
         owl:references ns:Paper[0,*] .

ns:Persona owl:authorId xsd:string[1,1] ;
           owl:semanticUrl xsd:string[1,1] .
